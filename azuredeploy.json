{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "IotHubs_IoTEdge_DevOps_iot_hub_name": {
            "defaultValue": "IoTEdgeDevOps-iothub",
            "type": "String"
        },
        "registries_IoTEdgeDevOps_name": {
            "defaultValue": "IoTEdgeDevOps",
            "type": "String"
        },
        "components_IoTEdge_DevOps_app_insights_name": {
            "defaultValue": "IoTEdgeDevOps-appinsights",
            "type": "String"
        },
        "provisioningServices_IoTEdgeDevOps_provisioning_name": {
            "defaultValue": "IoTEdgeDevOps-provisioning",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.ContainerRegistry/registries",
            "sku": {
                "name": "Standard",
                "tier": "Standard"
            },
            "name": "[parameters('registries_IoTEdgeDevOps_name')]",
            "apiVersion": "2017-10-01",
            "location": "eastus",
            "tags": {},
            "scale": null,
            "properties": {
                "adminUserEnabled": true
            },
            "dependsOn": []
        },
        {
            "type": "Microsoft.Devices/IotHubs",
            "sku": {
                "name": "S1",
                "tier": "Standard",
                "capacity": 1
            },
            "name": "[parameters('IotHubs_IoTEdge_DevOps_iot_hub_name')]",
            "apiVersion": "2018-04-01",
            "location": "eastus",
            "tags": {},
            "scale": null,
            "properties": {
                "operationsMonitoringProperties": {
                    "events": {
                        "None": "None",
                        "Connections": "None",
                        "DeviceTelemetry": "None",
                        "C2DCommands": "None",
                        "DeviceIdentityOperations": "None",
                        "FileUploadOperations": "None",
                        "Routes": "None"
                    }
                },
                "ipFilterRules": [],
                "eventHubEndpoints": {
                    "events": {
                        "retentionTimeInDays": 1,
                        "partitionCount": 2,
                        "partitionIds": [
                            "0",
                            "1"
                        ],
                        "path": "[concat(parameters('IotHubs_IoTEdge_DevOps_iot_hub_name'),'iotedge-devops-iot-hub', parameters('IotHubs_IoTEdge_DevOps_iot_hub_name'))]",
                        "endpoint": "sb://iothub-ns-iotedge-de-714666-25797ba1ac.servicebus.windows.net/"
                    },
                    "operationsMonitoringEvents": {
                        "retentionTimeInDays": 1,
                        "partitionCount": 2,
                        "partitionIds": [
                            "0",
                            "1"
                        ],
                        "path": "[concat(parameters('IotHubs_IoTEdge_DevOps_iot_hub_name'),'iotedge-devops-iot-hub-operationmonitoring')]",
                        "endpoint": "sb://iothub-ns-iotedge-de-714666-25797ba1ac.servicebus.windows.net/"
                    }
                },
                "routing": {
                    "endpoints": {
                        "serviceBusQueues": [],
                        "serviceBusTopics": [],
                        "eventHubs": [],
                        "storageContainers": []
                    },
                    "routes": [],
                    "fallbackRoute": {
                        "name": "$fallback",
                        "source": "DeviceMessages",
                        "condition": "true",
                        "endpointNames": [
                            "events"
                        ],
                        "isEnabled": true
                    }
                },
                "storageEndpoints": {
                    "$default": {
                        "sasTtlAsIso8601": "PT1H",
                        "connectionString": "",
                        "containerName": ""
                    }
                },
                "messagingEndpoints": {
                    "fileNotifications": {
                        "lockDurationAsIso8601": "PT1M",
                        "ttlAsIso8601": "PT1H",
                        "maxDeliveryCount": 10
                    }
                },
                "enableFileUploadNotifications": false,
                "cloudToDevice": {
                    "maxDeliveryCount": 10,
                    "defaultTtlAsIso8601": "PT1H",
                    "feedback": {
                        "lockDurationAsIso8601": "PT1M",
                        "ttlAsIso8601": "PT1H",
                        "maxDeliveryCount": 10
                    }
                },
                "features": "None"
            },
            "dependsOn": []
        },
        {
            "type": "Microsoft.Devices/provisioningServices",
            "sku": {
                "name": "S1",
                "tier": "Standard",
                "capacity": 1
            },
            "name": "[parameters('provisioningServices_IoTEdgeDevOps_provisioning_name')]",
            "apiVersion": "2017-11-15",
            "location": "eastus",
            "tags": {},
            "scale": null,
            "properties": {
                "iotHubs": []
            },
            "dependsOn": []
        },
        {
            "type": "microsoft.insights/components",
            "kind": "other",
            "name": "[parameters('components_IoTEdge_DevOps_app_insights_name')]",
            "apiVersion": "2015-05-01",
            "location": "eastus",
            "tags": {},
            "scale": null,
            "properties": {
                "Application_Type": "other",
                "Flow_Type": "Redfield",
                "Request_Source": "IbizaAIExtension",
                "HockeyAppId": null,
                "SamplingPercentage": null
            },
            "dependsOn": []
        }
    ]
}